{
    "name": "virtua-sa/doctrine1",
    "type": "library",
    "description": "PHP5+ Database ORM",
    "keywords": ["orm", "database"],
    "homepage": "http://www.doctrine-project.org",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {"name": "Konsta Vesterinen", "email": "kvesteri@cc.hut.fi"},
        {"name": "Jonathan Wage", "email": "jonwage@gmail.com"},
        {"name": "Rui Filipe Da Cunha Alves", "email": "ruifil@ruifil.com"}
    ],
    "autoload": {
        "psr-0": {
            "Doctrine_": "lib/",
            "sfYaml": "lib/Doctrine/Parser/sfYaml/"
        },
        "classmap": [
            "lib/Doctrine.php"
        ]
    },
    "replace": {
        "doctrine/doctrine1": "*",
        "diablomedia/doctrine1": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/diablomedia/phpstorm-stubs"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^4.0 || ^5.0",
        "jetbrains/phpstorm-stubs": "dev-phpstan"
    }
}
